"extends": "eslint:recommended",
 "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },

    "rules": {
            "strict": 2
        },
  "rules": {
    "no-console": 0,
     "no-mixed-spaces-and-tabs": [2, "smart-tabs"]
    },
    "globals": {
        "jQuery": true,
        "$": true,
        "console": true,
        "require": true,
        "module": true,
        "document": true
    }
}


